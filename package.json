{
  "name": "motoshop",
  "version": "1.0.0",
  "repository": "DmitryRomaniuk/motoshop",
  "author": "Dmitry Romaniuk",
  "license": "MIT",
  "browserslist": [
    "> 1%"
  ],
  "engines": {
    "node": "8.x",
    "yarn": "1.0.1"
  },
  "scripts": {
    "start": "npm run start-win & npm run start-linux",
    "start-win": "start npm run dev:start & npm run dev:wds",
    "start-linux": "npm run dev:start",
    "dev:start": "nodemon -e js,jsx --ignore build --ignore dist --exec babel-node src/server",
    "dev:wds": "webpack-dev-server --progress",
    "prod:build": "rimraf lib dist && babel src -d lib --ignore .test.js && cross-env NODE_ENV=production webpack -p --progress",
    "prod:start": "npm run prod:build && cross-env NODE_ENV=production heroku local",
    "lint": "eslint src webpack.config.babel.js --ext .js,.jsx",
    "fixlint": "npm run lint -- --fix",
    "flow": "flow",
    "test": "yarn lint && jest --coverage",
    "heroku-prebuild": "npm install",
    "heroku-postbuild": "yarn prod:build",
    "precommit": "yarn test",
    "prepush": "yarn test && yarn prod:build"
  },
  "dependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-flow-react-proptypes": "^24.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.2.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.0.0-beta",
    "compression": "^1.7.1",
    "cookie-parser": "^1.4.3",
    "coveralls": "^2.12.0",
    "cross-env": "^5.0.5",
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-flowtype": "^2.36.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "express": "^4.15.5",
    "express-session": "^1.15.6",
    "fetch-mock": "^5.9.4",
    "firebase-admin": "^5.3.0",
    "flow-bin": "^0.55.0",
    "flow-typed": "^2.1.5",
    "history": "^4.7.2",
    "husky": "^0.14.3",
    "immutable": "^3.8.1",
