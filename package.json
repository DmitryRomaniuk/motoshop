{
    "name": "motoshop",
    "version": "1.0.0",
    "repository": "DmitryRomaniuk/motoshop",
    "author": "Dmitry Romaniuk",
    "license": "MIT",
    "browserslist": [
        "> 1%"
    ],
    "engines": {
        "node": "8.x",
        "yarn": "1.0.1"
    },
    "scripts": {
        "start": "npm run start-win & npm run start-linux",
        "start-win": "start npm run dev:start & npm run dev:wds",
        "start-linux": "npm run dev:start",
        "dev:start": "nodemon -e js,jsx --ignore build --ignore dist --exec babel-node src/server",
        "dev:wds": "webpack-dev-server --progress",
        "prod:build": "rimraf lib dist && babel src -d lib --ignore .test.js && cross-env NODE_ENV=production webpack -p --progress",
        "prod:start": "npm run prod:build && cross-env NODE_ENV=production heroku local",
        "lint": "eslint src webpack.config.babel.js --ext .js,.jsx",
        "fixlint": "npm run lint -- --fix",
        "flow": "flow",
        "test": "yarn lint && jest --coverage",
        "heroku-prebuild": "npm install",
        "heroku-postbuild": "yarn prod:build",
        "precommit": "yarn test",
        "prepush": "yarn test && yarn prod:build"
    },
    "dependencies": {
        "babel-cli": "^6.23.0",
        "babel-core": "^6.23.1",
        "babel-eslint": "^8.0.0",
        "babel-jest": "^21.0.2",
        "babel-loader": "^7.1.2",
        "babel-plugin-flow-react-proptypes": "^5.1.2",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-polyfill": "^6.23.0",
        "babel-preset-env": "^1.2.0",
        "babel-preset-flow": "^6.23.0",
        "babel-preset-react": "^6.23.0",
        "body-parser": "^1.18.1",
        "bootstrap": "4.0.0-alpha.6",
        "compression": "^1.6.2",
        "cookie-parser": "^1.4.3",
        "coveralls": "^2.12.0",
        "cross-env": "^5.0.5",
        "dotenv": "^4.0.0",
        "eslint": "^4.6.1",
        "eslint-config-airbnb": "^15.1.0",
        "eslint-plugin-compat": "^1.0.2",
        "eslint-plugin-flowtype": "^2.30.0",
        "eslint-plugin-import": "^2.2.0",
        "eslint-plugin-jsx-a11y": "^5.0.1",
        "eslint-plugin-react": "^7.3.0",
        "express": "^4.15.0",
        "fetch-mock": "^5.9.4",
        "firebase-admin": "^5.2.1",
        "flow-bin": "^0.54.1",
        "flow-typed": "^2.1.5",
        "husky": "^0.14.3",
        "immutable": "^3.8.1",
        "isomorphic-fetch": "^2.2.1",
        "jest": "^21.0.2",
        "jquery": "^3.1.1",
        "mongoose": "^4.9.0",
        "nodemon": "^1.11.0",
        "prop-types": "^15.5.10",
        "react": "^15.4.2",
        "react-dom": "^15.4.2",
        "react-helmet": "^5.2.0",
        "react-hot-loader": "^3.0.0-beta.6",
        "react-jss": "^7.1.0",
        "react-redux": "^5.0.3",
        "react-router": "4.2.0",
        "react-router-dom": "4.2.2",
        "redux": "^3.6.0",
        "redux-actions": "^2.0.1",
        "redux-form": "^7.0.4",
        "redux-mock-store": "^1.2.2",
        "redux-thunk": "^2.2.0",
        "rimraf": "^2.6.1",
        "serve-favicon": "^2.4.1",
        "socket.io": "^2.0.3",
        "socket.io-client": "^2.0.3",
        "tether": "^1.4.0",
        "webpack": "^2.2.1",
        "webpack-dev-server": "^2.6.1"
    },
    "devDependencies": {}
}
