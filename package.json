    "jest": "^21.2.1",
    "jquery": "^3.1.1",
    "jsonwebtoken": "^8.1.0",
    "mongoose": "^4.11.13",
    "nodemon": "^1.11.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "popper.js": "^1.12.5",
    "prop-types": "^15.6.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.1.3",
    "react-jss": "^8.4.0",
